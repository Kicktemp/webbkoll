<section class="result">
  <h3 id="raw-headers"><%= gettext "Raw headers" %></h3>

  <table>
    <thead>
      <tr>
        <th>Header</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      <%= for {header, value} <- @site.headers do %>
      <tr>
        <td><%= header %></td>
        <td><%= value %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</section>